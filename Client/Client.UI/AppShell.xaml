<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="Client.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="using:Client.UI.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Client.UI">

    <ShellContent
        Title="PlatformPage"
        ContentTemplate="{DataTemplate views:PlatformPage}"
        Route="PlatformPage" />

    <ShellContent
        Title="LoginPage"
        ContentTemplate="{DataTemplate views:LoginPage}"
        Route="LoginPage" />


</Shell>