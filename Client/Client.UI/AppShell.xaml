<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="Client.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="using:Client.UI.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Client.UI">

    <ShellContent
        Title="LoginPage"
        ContentTemplate="{DataTemplate views:LoginPage}"
        Route="LoginPage" />

    <ShellContent
        Title="NewUserPage"
        ContentTemplate="{DataTemplate views:NewUserPage}"
        Route="NewUserPage" />

    <ShellContent
        Title="Test"
        ContentTemplate="{DataTemplate views:TestPage}"
        Route="TestPage" />

    <ShellContent
        Title="PlatormPage"
        ContentTemplate="{DataTemplate views:PlatformPage}"
        Route="Platform" />

    <ShellContent
        Title="Join"
        ContentTemplate="{DataTemplate views:JoinPage}"
        Route="Join" />

    <ShellContent
        Title="Lobby"
        ContentTemplate="{DataTemplate views:LobbyPage}"
        Route="Platform" />
    
  


</Shell>